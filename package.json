  "bugs": {
    "url": "https://github.com/NguyenVanPhi92/trello-app-api/issues"
  },
  "homepage": "https://github.com/NguyenVanPhi92/trello-app-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "mongodb": "^4.8.0",
    "nodemon": "^2.0.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
